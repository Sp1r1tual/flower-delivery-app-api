{
  "name": "flower-delivery-app-api",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Andrii Zub",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "node --loader ts-node/esm ./src/index.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^5.1.0",
    "mongoose": "^8.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^11.1.0",
    "typescript": "^5.9.2"
  }
}
